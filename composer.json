{
    "autoload": {
        "psr-4": {
            "App\\": "./lib/App/src/",
            "Core\\": "./lib/Core/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\Fixtures\\": "./lib/App/test/fixtures/",
            "App\\Test\\Integration\\": "./lib/App/test/integration/",
            "App\\Test\\Unit\\": "./lib/App/test/unit/",
            "Core\\": "../core/Core/src/",
            "Dev\\": "../core/Core/dev/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        },
        "process-timeout": 0,
        "sort-packages": true,
        "vendor-dir": "./lib/vendor"
    },
    "require": {
        "bryanjhv/slim-session": "~4.0",
        "erusev/parsedown": "^1.7",
        "ezyang/htmlpurifier": "^4.10",
        "google/apiclient": "^2.0",
        "middlewares/trailing-slash": "^2.0",
        "monolog/monolog": "^2.0",
        "pelago/emogrifier": "^5.0",
        "php-di/php-di": "^6.4",
        "phpmailer/phpmailer": "^6.0",
        "phpmussel/phpmussel": "^3.3",
        "rlanvin/php-rrule": "^2.2",
        "slim/csrf": "^1.0",
        "slim/flash": "^0.4.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.0",
        "slim/twig-view": "^3.2"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-zombie-driver": "^1.4",
        "fzaninotto/faker": "^1.6",
        "php-mock/php-mock-phpunit": "^2.6",
        "phpmetrics/phpmetrics": "^2.4",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^9.0"
    }
}
